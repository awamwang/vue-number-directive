{
  "name": "vue-number-directive",
  "version": "0.0.2",
  "description": "Vue directive plugin to assure number to be number",
  "author": {
    "name": "Awam M Wang",
    "email": "keepgoingwm@gmail.com"
  },
  "scripts": {
    "dev": "parcel watch src/index.js --public-url ./dist/",
    "storybook": "start-storybook -p 6006",
    "cy-open": "cypress open -C cypress.json",
    "build": "npm run build-cjs && npm run build-umd && npm run build-storybook",
    "build-cjs": "parcel build src/index.js",
    "build-umd": "parcel build --global VueNumber src/index.js",
    "build-storybook": "build-storybook -o ./docs/storybook",
    "lint": "vue-cli-service lint",
    "cy-run": "cypress run",
    "init-commitizen": "commitizen init cz-conventional-changelog --save",
    "init-storybook": "npx sb init",
    "coverage-nyc_setup": "nyc instrument --compact=false src test/coverage/instrumented",
    "coverage-nyc_report": "nyc report --reporter=text-summary",
    "test": "npm run cy-run && npm run coverage-nyc_report"
  },
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "src/index.js",
  "browser": "dist/index.umd.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@cypress/code-coverage": "^3.8.1",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-docs": "^6.1.0-alpha.14",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-storysource": "^6.0.21",
    "@storybook/cli": "^6.0.21",
    "@storybook/vue": "^6.0.21",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-app": "^4.5.6",
    "@vue/babel-preset-jsx": "^1.1.2",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^5.2.0",
    "element-ui": "^2.13.2",
    "nyc": "^15.1.0",
    "parcel": "^2.0.0-beta.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 8"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ],
    "ssr": [
      "node 8"
    ]
  },
  "bugs": {
    "url": "https://github.com/keepgoingwm/vue-number-directive/issues"
  },
  "engines": {
    "node": ">=8"
  },
  "homepage": "https://github.com/keepgoingwm/vue-number-directive#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keepgoingwm/vue-number-directive.git"
  }
}
