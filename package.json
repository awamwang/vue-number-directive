{
  "name": "vue-number-directive",
  "version": "0.0.4-0",
  "description": "Vue directive plugin to assure number to be number",
  "author": {
    "name": "Awam M Wang",
    "email": "keepgoingwm@gmail.com"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run dev\" \"npm run storybook\"",
    "storybook": "start-storybook -p 6006",
    "cy-open": "cypress open -C cypress.json",
    "test": "npm run cy-run && npm run coverage-nyc_report",
    "dev": "parcel watch src/index.js --public-url ./dist/",
    "version": "npm run build && npm run changelog && git add CHANGELOG.md",
    "build": "npm run build-cjs && npm run build-umd",
    "lint": "vue-cli-service lint",
    "build-cjs": "parcel build src/index.js",
    "build-umd": "parcel build --global VueNumber src/index.umd.js",
    "build-storybook": "build-storybook -o ./docs/docs",
    "build-readme": "gh-md-toc --insert README.md && gh-md-toc --insert README.zh_CN.md && rm README*.md.orig.* && rm README*.md.toc.*",
    "build-doc": "npm run build-storybook",
    "cy-run": "cypress run",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "release-github": "conventional-github-releaser -p angular -r 1",
    "clean": "rimraf ./dist && mkdir dist",
    "prebuild": "npm run clean",
    "coverage-nyc_report": "nyc report --reporter=text-summary"
  },
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "src/index.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@cypress/code-coverage": "^3.8.1",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-docs": "^6.1.0-alpha.14",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-storysource": "^6.0.21",
    "@storybook/cli": "^6.0.21",
    "@storybook/vue": "^6.0.21",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-app": "^4.5.6",
    "@vue/babel-preset-jsx": "^1.1.2",
    "babel-plugin-istanbul": "^6.0.0",
    "concurrently": "^5.3.0",
    "coveralls": "^3.1.0",
    "cypress": "^5.3.0",
    "element-ui": "^2.13.2",
    "husky": "^4.3.0",
    "nyc": "^15.1.0",
    "parcel": "^2.0.0-nightly.410",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-wrapper": "^0.2.3",
    "prettier": "^1.19.1",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 8"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ],
    "ssr": [
      "node 8"
    ]
  },
  "bugs": {
    "url": "https://github.com/awamwang/vue-number-directive/issues"
  },
  "engines": {
    "node": ">=8"
  },
  "homepage": "https://awamwang.github.io/vue-number-directive/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awamwang/vue-number-directive.git"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
